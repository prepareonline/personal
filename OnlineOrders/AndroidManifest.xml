<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manulogix.rbash"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_owner_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.manulogix.rbash.owner.WelcomeScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.manulogix.rbash.owner.CustomersListActivity" >
        </activity>
        <activity android:name="com.manulogix.rbash.owner.MainActivity" />
        <activity android:name="com.manulogix.rbash.owner.OrdersListActivity" >
        </activity>
    </application>

</manifest>